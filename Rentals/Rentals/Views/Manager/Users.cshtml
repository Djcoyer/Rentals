@model List<Rentals.Models.ApplicationUser>
@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>

<div class="container">

    <div class="row">
        <div class="col-md-2">
            <h4>First Name</h4>
        </div>
        <div class="col-md-2">
            <h4>Last Name</h4>
        </div>
    </div>
    <hr/>
    @foreach (var user in Model)
    {
        <div class="row">
            <div class="col-md-1">
                <strong>@user.FirstName</strong>
            </div>
            <div class="col-md-1">
                |
            </div>
            <div class="col-md-1">
                <strong>@user.LastName</strong>
            </div>
            <div class="col-md-1">
                <input type="submit" id="editButton" value="Edit User" class="btn btn-primary" />
            </div>
        </div>
        <hr />
    }


</div>

